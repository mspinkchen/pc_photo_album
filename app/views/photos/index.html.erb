<div class = "my container container">

<div class="photolist">
  <h1>Add New Photo</h1>
<table class="table table-striped table-condensed">

  <% @photos.each do |photo| %>
  <tr>
  <td><%= photo.title %></td>
  <td><%= link_to 'show', photo_path(photo) %></td>
  <td><%= link_to 'edit', edit_photo_path(photo) %></td>
  <td><%= link_to 'delete', photo_path(photo), method: :delete, data: {confirm: "are you sure?"} %></td>
  </tr>
  <% end %>

  </ul>

</table>

<%= link_to 'New Photo', new_photo_path, class: "btn btn-info" %>
</div>
</div>